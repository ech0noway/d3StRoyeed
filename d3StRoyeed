  </style>
<script>
  const text = "Дестроид 🥶😈😈😈";
  let i = 0;
  const speed = 70;

  function typeWriter() {
    if (i < text.length) {
      document.getElementById("typing").innerHTML += text.charAt(i);
      i++;
      setTimeout(typeWriter, speed);
    }
  }
<img src="d3.png" alt="WOW" width="300"/>
  typeWriter();
</script>  
